<launch>
  <param name="/use_sim_time" value="true"/>
  <param name="robot_description" command="xacro $(find ansrrbot)/xacro/ansrr_v2.xacro"/>
  <include file="$(find gazebo_ros)/launch/empty_world.launch"/>
  <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model"
    args="-param robot_description -urdf -model ansrr_bot"/>
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"/>
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find ansrrbot)/config/test_viz.rviz"/>
</launch>
